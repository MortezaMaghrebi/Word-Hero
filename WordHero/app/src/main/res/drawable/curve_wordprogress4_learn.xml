            <solid android:color="#0000b091"/>
            <stroke android:width="10dp" android:color="#a01e2510" />
            <corners android:radius="15dp"/>
            <padding android:left="0dp" android:top="0dp" android:right="0dp" android:bottom="0dp" />
        </shape>
    </item>
</layer-list>
